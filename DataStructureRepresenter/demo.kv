ScreenManager:
    Dselecter:
    Stack:
    Queue:
    LinkedList:

<Dselecter>:
    name: 'ds'

    MDRectangleFlatButton:
        text: 'Stack'
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        on_press:
            root.manager.current = 'stack'

    MDRectangleFlatButton:
        text: 'Queue'
        pos_hint: {'center_x': 0.5, 'center_y': 0.4}
        on_press:
            root.manager.current = 'queue'

    MDRectangleFlatButton:
        text: 'Linked list'
        pos_hint: {'center_x': 0.5, 'center_y':0.3}
        on_press:
            root.manager.current = 'll'
<Stack>:
    name: 'stack'
    id: stack_class
    label : content_holder
    textfield : textfield

    GridLayout:
        cols: 1

        MDTextField:
            id: textfield
            pos_hint: {'center_x': 0, 'center_y': 0.9}
            size_hint_x: None
            width: 60

        MDRectangleFlatButton:
            text: 'go!'
            size: (20, 25)
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            on_release:
                root.append_stack()

        MDRectangleFlatButton:
            text: 'Pop'
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            size: (20, 25)
            on_press:
                root.pop_stack()

        MDLabel:
            id: content_holder
            halign: 'center'
            valign: 'middle'

        MDRoundFlatButton:
            text: 'Back'
            on_press:
                root.manager.current = 'ds'

<Queue>:
    name: 'queue'
    q_textfield: q_textfield
    q_label: q_label
    GridLayout:
        cols: 1
        MDTextField:
            id: q_textfield
            pos_hint: {'center_x': 0, 'center_y': 0.9}
            size_hint_x: None
            width: 60

        MDRectangleFlatButton:
            text: 'go!'
            size: (20, 25)
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            on_release:
                root.append_queue()

        MDRectangleFlatButton:
            text: 'Pop'
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            size: (20, 25)
            on_press:
                root.pop_queue()

        MDLabel:
            id: q_label
            halign: 'center'
            valign: 'middle'

        MDRoundFlatButton:
            text: 'Back'
            on_press:
                root.manager.current = 'ds'
<LinkedList>:
    name : 'll'